<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="470dp"
    android:layout_alignParentBottom="true">

    <Button
        android:id="@+id/button_16"
        android:layout_width="70dp"
        android:layout_height="150dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="16dp"
        android:text="="
        android:textSize="50dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_19"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toLeftOf="@id/button_16"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="16dp"
        android:text="."
        android:gravity="center"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_18"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toRightOf="@id/button_17"
        android:layout_marginLeft="16dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:text="0"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_17"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="16dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:text="%"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_15"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_19"
        android:layout_toLeftOf="@id/button_16"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text="3"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_13"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_17"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="1"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_14"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_18"
        android:layout_toRightOf="@id/button_13"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="16dp"
        android:text="2"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_12"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_16"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text="+"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_11"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toLeftOf="@id/button_12"
        android:layout_above="@id/button_15"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text="6"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_9"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_13"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="4"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_10"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toRightOf="@id/button_9"
        android:layout_above="@id/button_14"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="5"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_8"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentRight="true"
        android:layout_above="@id/button_12"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text="-"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_7"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toLeftOf="@id/button_8"
        android:layout_above="@id/button_11"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text="9"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_9"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="7"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_6"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toRightOf="@id/button_5"
        android:layout_above="@id/button_10"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="8"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_4"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentRight="true"
        android:layout_above="@id/button_8"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text="del"
        android:textSize="15dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toLeftOf="@id/button_4"
        android:layout_above="@id/button_7"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text="*"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_1"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_5"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="C"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_toRightOf="@id/button_1"
        android:layout_above="@id/button_6"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="/"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_20"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_above="@id/button_1"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="10dp"
        android:text="("
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

    <Button
        android:id="@+id/button_21"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_above="@id/button_4"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="10dp"
        android:text=")"
        android:textSize="25dp"
        android:background="#00000000"
        android:textColor="#ffffffff"
        />

</RelativeLayout>